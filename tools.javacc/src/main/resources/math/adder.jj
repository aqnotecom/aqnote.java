/** adder.jj Adding up numbers */options{  STATIC = false; 	//语法分析器是否为静态  LOOKAHEAD = 1;	//LL(1)文法}PARSER_BEGIN(Adder)package com.aqnote.shared.javacc.math.adder;import java.io.ByteArrayInputStream;import java.io.InputStream;public class Adder{  public static void main(String [] args) throws ParseException, TokenMgrError  {    //    String value = "123+1-5";    //    InputStream is = new ByteArrayInputStream(value.getBytes());    Adder parser = new Adder(System.in);    int myResult = parser._start();    System.out.println(myResult);  }}PARSER_END(Adder)/*跳过的字符*/SKIP:{  " "| "\n"| "\r"| "\r\n"}/*单词*/TOKEN:{  < PLUS : "+" >}TOKEN:{  < MINUS : "-" >}TOKEN:{  < NUMBER : ([ "0"-"9" ])+ >}int _start() :{  int value;  int result;}{  result = _primary()  (    < PLUS > value = _primary()    {      result += value;    }  | < MINUS > value = _primary()    {      result -= value;    }  )*  < EOF >  {    return result;  }}int _primary() throws NumberFormatException :{  Token tmpToken;}{  tmpToken = < NUMBER >  {    return Integer.parseInt(tmpToken.image);  }}