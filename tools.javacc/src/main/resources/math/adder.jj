/** adder.jj Adding up numbers */options{  STATIC = false;}PARSER_BEGIN(Adder)package com.madding.shared.javacc.math.adder;import java.io.ByteArrayInputStream;import java.io.InputStream;public class Adder{  public static void main(String [] args) throws ParseException, TokenMgrError  {    //    String value = "123+1-5";    //    InputStream is = new ByteArrayInputStream(value.getBytes());    Adder parser = new Adder(System.in);    int myResult = parser._start();    System.out.println(myResult);  }}PARSER_END(Adder)SKIP :{  " "}SKIP :{  "\n"| "\r"| "\r\n"}TOKEN :{  < PLUS : "+" >}TOKEN :{  < MINUS : "-" >}TOKEN :{  < NUMBER : ([ "0"-"9" ])+ >}int _start() :{  int value;  int result;}{  result = _primary()  (    < PLUS > value = _primary()    {      result += value;    }  | < MINUS > value = _primary()    {      result -= value;    }  )*  < EOF >  {    return result;  }}int _primary() throws NumberFormatException :{  Token tmpToken;}{  tmpToken = < NUMBER >  {    return Integer.parseInt(tmpToken.image);  }}