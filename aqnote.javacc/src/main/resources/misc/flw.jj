/* flw.jj A simple report writer. */options{  static = false;}PARSER_BEGIN(FLW)package com.madding.shared.javacc.misc.flw;import java.io.Reader;import java.io.StringReader;class FLW{  public static void main(String[] args) {    System.out.println(FLW.substitute("abcd abddcc dddd ddddd"));  }      public static String substitute(String inString)  {    Reader reader = new StringReader(inString);    FLW parser = new FLW(reader);    StringBuffer buffer = new StringBuffer();    try    {      parser._parse(buffer);    }    catch (TokenMgrError e)    {      throw new IllegalStateException();    }    catch (ParseException e)    {      throw new IllegalStateException();    }    return buffer.toString();  }}PARSER_END(FLW)SKIP :{  " "| "\r"| "\n"| "\r\n"}TOKEN :{  < FOUR_LETTER_WORD : (< LETTER >)    {      4    }    >}TOKEN :{  < FIVE_LETTER_WORD : (< LETTER >)    {      5    }    (< LETTER >)* >}TOKEN :{  < #DIGIT : [ "0"-"9" ] >}TOKEN :{  < #LETTER : [ "a"-"z", "A"-"Z" ] >}TOKEN :{  < OTHER : ~[ ] >}void _parse(StringBuffer buffer) :{  Token tmpToken;}{  (    < FOUR_LETTER_WORD >    {      buffer.append("****");    }  |    (      tmpToken = < FIVE_LETTER_WORD >    | tmpToken = < OTHER >    )    {      buffer.append(tmpToken.image);    }  )*  < EOF >}